{
    "name": "kmuharam/tamex-php",
    "description": "",
    "keywords": [
        "kmuharam",
        "tamex-php"
    ],
    "homepage": "https://github.com/kmuharam/tamex-php",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Khalid Muharam",
            "email": "devel@muharam.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Kmuharam\\TamexPhp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kmuharam\\TamexPhp\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
